{
  "name": "sariyanta",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/sariyanta/app-sariyanta-nestjs.git",
  "author": "Desar <desar@sariyanta.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn workspaces foreach -A --parallel -i run build",
    "start": "yarn workspace server start:prod",
    "dev": "yarn workspaces foreach -A --parallel -i run dev",
    "test": "yarn workspaces run test",
    "test:e2e": "yarn workspaces run test:e2e",
    "create:env:server": "printenv > ./apps/server/.env"
  },
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "@yarnpkg/cli": "^4.9.1",
    "@yarnpkg/core": "^4.4.1",
    "@yarnpkg/plugin-git": "^3.1.1",
    "@yarnpkg/plugin-workspace-tools": "^4.1.4"
  },
  "packageManager": "yarn@4.9.1"
}